<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.tcs.demo.entities.VehicleLocation" table="VehicleLocation">
      <composite-id>
		<key-property name="vehicleId" column="VehicleID"/>
		<key-property name="latitude" column="Latitude"/>
		<key-property name="longitude" column="Longitude"/>
		<key-property name="locationChangedOn" column="LocationChangedOn"/>
	  </composite-id>
      <!-- <property name="subscriptionAcknowledged" column="SubscriptionAcknowledged"/> -->
   </class>
</hibernate-mapping>
